/projects/dog-walk/client/src/components/petlist/index.js",[],"/Users/mallorysheehan/projects/dog-walk/client/src/components/petform/index.js",["50"],"import React, { useState } from \"react\";\nimport { Link } from \"react-router-dom\";\nimport { useMutation } from \"@apollo/client\";\n\nimport { ADD_PET } from \"../../utils/mutations\";\nimport { QUERY_PETS, QUERY_ME } from \"../../utils/queries\";\n\nimport Auth from \"../../utils/auth\";\n\nconst PetForm = () => {\n  const [petName, setPetName] = useState(\"\");\n\n  const [characterCount, setCharacterCount] = useState(0);\n\n  const [addPet, { error }] = useMutation(ADD_PET, {\n    update(cache, { data: { addPet } }) {\n      try {\n        const { pets } = cache.readQuery({ query: QUERY_PETS });\n\n        cache.writeQuery({\n          query: QUERY_PETS,\n          data: { pets: [addPet, ...pets] },\n        });\n      } catch (e) {\n        console.error(e);\n      }\n\n      // update me object's cache\n      const { me } = cache.readQuery({ query: QUERY_ME });\n      cache.writeQuery({\n        query: QUERY_ME,\n        data: { me: { ...me, pets: [...me.pets, addPet] } },\n      });\n    },\n  });\n\n  const handleFormSubmit = async (event) => {\n    event.preventDefault();\n\n    try {\n      const { data } = await addPet({\n        variables: {\n          petName,\n          petUser: Auth.getProfile().data.userFullname,\n        },\n      });\n\n      setPetName(\"\");\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  const handleChange = (event) => {\n    const { name, value } = event.target;\n\n    if (name === \"petName\" && value.length <= 280) {\n      setPetName(value);\n      setCharacterCount(value.length);\n    }\n  };\n\n  return (\n    <div>\n      <h3>Who would you like for us to walk?</h3>\n\n      {Auth.loggedIn() ? (\n        <>\n          <p\n            className={`m-0 ${\n              characterCount === 280 || error ? \"text-danger\" : \"\"\n            }`}\n          >\n            Character Count: {characterCount}/280\n          </p>\n          <form\n            className=\"flex-row justify-center justify-space-between-md align-center\"\n            onSubmit={handleFormSubmit}\n          >\n            <div className=\"col-12 col-lg-9\">\n              <textarea\n                name=\"petName\"\n                placeholder=\"My dog's name is...\"\n                value={petName}\n                className=\"form-input w-100\"\n                style={{ lineHeight: \"1.5\", resize: \"vertical\" }}\n                onChange={handleChange}\n              ></textarea>\n            </div>\n\n            <div className=\"col-12 col-lg-3\">\n              <button className=\"btn btn-primary btn-block py-3\" type=\"submit\">\n                Add Pet\n              </button>\n            </div>\n            {error && (\n              <div className=\"col-12 my-3 bg-danger text-white p-3\">\n                {error.message}\n              </div>\n            )}\n          </form>\n        </>\n      ) : (\n        <p>\n          You need to be logged in to add your pet/s. Please{\" \"}\n          <Link to=\"/login\">login</Link> or <Link to=\"/signup\">signup.</Link>\n        </p>\n      )}\n    </div>\n  );\n};\n\nexport default PetForm;\n",{"ruleId":"51","replacedBy":"52"},{"ruleId":"53","replacedBy":"54"},{"ruleId":"55","severity":1,"message":"56","line":41,"column":15,"nodeType":"57","messageId":"58","endLine":41,"endColumn":19},"no-native-reassign",["59"],"no-negated-in-lhs",["60"],"no-unused-vars","'data' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]