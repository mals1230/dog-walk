n(ADD_WALK);\n\n  const handleFormSubmit = async (event) => {\n    event.preventDefault();\n\n    try {\n      const { data } = await addWalk({\n        variables: {\n          petUser: Auth.getProfile().data.userFullName,\n          walkDate,\n          walkTime,\n          walkDuration,\n          dogWalker,\n          pet\n        },\n      });\n\n      setWalkDate(\"\");\n      setWalkTime(\"\");\n      setWalkDuration(\"\");\n      setDogWalker(\"\");\n      setPet(\"\");\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  const handleChange = (event) => {\n    const { name, value } = event.target;\n\n    if (name === \"walkDate\" && value.length <= 280) {\n      setWalkDate(value);\n    } if (name === \"walkTime\" && value.length <= 280) {\n      setWalkTime(parseInt(value));\n    } if (name === \"walkDuration\" && value.length <= 280) {\n      setWalkDuration(parseInt(value));\n    } if (name === \"dogWalker\" && value.length <= 280) {\n      setDogWalker(value);\n    } if (name === \"petName\" && value.length <= 280) {\n      setPet(value);\n    }\n  };\n\n\n  return (\n    <div>\n\n      {Auth.loggedIn() ? (\n        <>\n          <h3>Book A Walk!</h3>\n         \n          <form\n            className=\"flex-row justify-center justify-space-between-md align-center\"\n            onSubmit={handleFormSubmit}\n          >\n            <div className=\"col-12 col-lg-9\">\n              <textarea\n                name=\"walkDate\"\n                placeholder=\"Date of walk request\"\n                value={walkDate}\n                className=\"form-input w-100\"\n                style={{ lineHeight: '1.5', resize: 'vertical' }}\n                onChange={handleChange}\n              ></textarea>\n            </div>\n            <div className=\"col-12 col-lg-9\">\n              <textarea\n                name=\"walkTime\"\n                placeholder=\"Time of walk request\"\n                value={walkTime}\n                className=\"form-input w-100\"\n                style={{ lineHeight: '1.5', resize: 'vertical' }}\n                onChange={handleChange}\n              ></textarea>\n            </div>\n            <div className=\"col-12 col-lg-9\">\n              <textarea\n                name=\"walkDuration\"\n                placeholder=\"requested duration\"\n                value={walkDuration}\n                className=\"form-input w-100\"\n                style={{ lineHeight: '1.5', resize: 'vertical' }}\n                onChange={handleChange}\n              ></textarea>\n            </div>\n            <div className=\"col-12 col-lg-9\">\n              <textarea\n                name=\"dogWalker\"\n                placeholder=\"requested walker\"\n                value={dogWalker}\n                className=\"form-input w-100\"\n                style={{ lineHeight: '1.5', resize: 'vertical' }}\n                onChange={handleChange}\n              ></textarea>\n            </div>\n            <div className=\"col-12 col-lg-9\">\n              <textarea\n                name=\"pet\"\n                placeholder=\"name of pet to be walked\"\n                value={pet}\n                className=\"form-input w-100\"\n                style={{ lineHeight: '1.5', resize: 'vertical' }}\n                onChange={handleChange}\n              ></textarea>\n            </div>\n\n            <div className=\"col-12 col-lg-3\">\n              <button className=\"btn btn-primary btn-block py-3\" type=\"submit\">\n                Add Walk\n              </button>\n            </div>\n            {/* {error && (\n              <div className=\"col-12 my-3 bg-danger text-white p-3\">\n                {error.message}\n              </div>\n            )} */}\n          </form>\n        </>\n      ) : (\n        <p>\n          You need to be logged in to book a walk. Please{' '}\n          <Link to=\"/login\">login</Link> or <Link to=\"/signup\">signup.</Link>\n        </p>\n      )}\n    </div>\n  );\n}