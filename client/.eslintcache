ata: { me: { ...me, pets: [...me.pets, addPet] } },\n      });\n    },\n  });\n\n  const handleFormSubmit = async (event) => {\n    event.preventDefault();\n\n    try {\n      const { data } = await addPet({\n        variables: {\n          petName,\n          petBreed,\n          petAge,\n          petWeight,\n          petInstructions,\n          petEmergency,\n          petUser: Auth.getProfile().data.userFullName,\n        },\n      });\n\n      setPetName(\"\");\n      setPetBreed(\"\");\n      setAge(\"\");\n      setWeight(\"\");\n      setInstructions(\"\");\n      setEmergency(\"\");\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  const handleChange = (event) => {\n    const { name, value } = event.target;\n\n    if (name === \"petName\" && value.length <= 280) {\n      setPetName(value);\n      setCharacterCount(value.length);\n    }\n  };\n\n  return (\n    <div>\n      <h3>Who would you like for us to walk?</h3>\n\n      {Auth.loggedIn() ? (\n        <>\n          <form\n            className=\"flex-row justify-center justify-space-between-md align-center\"\n            onSubmit={handleFormSubmit}\n          >\n            <div className=\"col-12 col-lg-9\">\n              <textarea\n                name=\"petName\"\n                placeholder=\"My dog's name is...\"\n                value={petName}\n                className=\"form-input w-100\"\n                style={{ lineHeight: \"1.5\", resize: \"vertical\" }}\n                onChange={handleChange}\n              ></textarea>\n            </div>\n            <div className=\"col-12 col-lg-9\">\n              <textarea\n                name=\"petBreed\"\n                placeholder=\"My dog's breed is ... \"\n                value={petBreed}\n                className=\"form-input w-100\"\n                style={{ lineHeight: \"1.5\", resize: \"vertical\" }}\n                onChange={handleChange}\n              ></textarea>\n            </div>\n            <div className=\"col-12 col-lg-9\">\n              <textarea\n                name=\"petAge\"\n                placeholder=\"Age of dog\"\n                value={petAge}\n                className=\"form-input w-100\"\n                style={{ lineHeight: \"1.5\", resize: \"vertical\" }}\n                onChange={handleChange}\n              ></textarea>\n            </div>\n            <div className=\"col-12 col-lg-9\">\n              <textarea\n                name=\"petWeight\"\n                placeholder=\"Weight of dog\"\n                value={petWeight}\n                className=\"form-input w-100\"\n                style={{ lineHeight: \"1.5\", resize: \"vertical\" }}\n                onChange={handleChange}\n              ></textarea>\n            </div>\n            <div className=\"col-12 col-lg-9\">\n              <textarea\n                name=\"petInstructions\"\n                placeholder=\"Address, Entry Instructions, Pet care instructions\"\n                value={petInstructions}\n                className=\"form-input w-100\"\n                style={{ lineHeight: \"1.5\", resize: \"vertical\" }}\n                onChange={handleChange}\n              ></textarea>\n            </div>\n            <div className=\"col-12 col-lg-9\">\n              <textarea\n                name=\"petEmergency\"\n                placeholder=\"Emergency info:\"\n                value={petEmergency}\n                className=\"form-input w-100\"\n                style={{ lineHeight: \"1.5\", resize: \"vertical\" }}\n                onChange={handleChange}\n              ></textarea>\n            </div>\n\n            <div className=\"col-12 col-lg-3\">\n              <button className=\"btn btn-primary btn-block py-3\" type=\"submit\">\n                Add Pet\n              </button>\n            </div>\n            {error && (\n              <div className=\"col-12 my-3 bg-danger text-white p-3\">\n                {error.message}\n              </div>\n            )}\n          </form>\n        </>\n      ) : (\n        <p>\n          You need to be logged in to add your pet/s. Please{\" \"}\n          <Link to=\"/login\">login</Link> or <Link to=\"/signup\">signup.</Link>\n        </p>\n      )}\n    </div>\n  );\n};\n\nexport default PetForm;\n",{"ruleId":"76","replacedBy":"77"},{"ruleId":"78","replacedBy":"79"},{"ruleId":"80","severity":1,"message":"81","line":41,"column":15,"nodeType":"82","messageId":"83","endLine":41,"endColumn":19},{"ruleId":"76","replacedBy":"84"},{"ruleId":"78","replacedBy":"85"},{"ruleId":"80","severity":1,"message":"86","line":16,"column":10,"nodeType":"82","messageId":"83","endLine":16,"endColumn":24},{"ruleId":"80","severity":1,"message":"81","line":44,"column":15,"nodeType":"82","messageId":"83","endLine":44,"endColumn":19},"no-native-reassign",["87"],"no-negated-in-lhs",["88"],"no-unused-vars","'data' is assigned a value but never used.","Identifier","unusedVar",["87"],["88"],"'characterCount' is assigned a value but never used.","no-global-assign","no-unsafe-negation"]